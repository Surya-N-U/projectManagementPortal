<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Students by Faculty</title>
</head>
<body>
    <h1>Students by Faculty</h1>
    <h2>Faculty ID: <%= data.facultyID %></h2>
    
    <h3>Groups:</h3>
    <ul>
        <% data.groups.forEach(function(group) { %>
            <li>Group ID: <%= group.groupID %></li>
            <ul>
                <% Object.keys(group.students).forEach(function(studentKey) { %>
                    <li><%= studentKey %>: <%= group.students[studentKey] %></li>
                <% }); %>
            </ul>
        <% }); %>
    </ul>
</body>
</html>

